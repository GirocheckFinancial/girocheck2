<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.smartbt.girocheck.servercommon.model.ReportFilters" table="Report_Filters" lazy="false">
        <id name="id" column="id" type="integer" unsaved-value="0">
            <generator class="native">
            </generator>
        </id>
        <property name="startRangeDate" column="start_range_date" type="string" length="255" not-null="false" lazy="false"/>
        <property name="endRangeDate" column="end_range_date" type="string" length="255" not-null="false" lazy="false"/>
        <property name="amount" column="amount" type="string" length="20" not-null="false" lazy="false"/>
        <property name="token" column="token" type="string" length="255" not-null="false" lazy="false"/>
        <property name="createAt" column="create_at" type="timestamp" not-null="false" lazy="false"/>
        <property name="entityId" column="entity_id" type="integer" not-null="false" lazy="false"/>
  
    </class>
</hibernate-mapping>

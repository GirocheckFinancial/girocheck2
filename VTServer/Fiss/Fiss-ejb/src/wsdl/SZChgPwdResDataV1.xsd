						<xs:documentation>New Password</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E300206" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Application ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E300207" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Terminal ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E300208" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Alternate Routing ID</xs:documentation>

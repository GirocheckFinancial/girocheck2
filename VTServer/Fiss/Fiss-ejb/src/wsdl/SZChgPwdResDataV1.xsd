<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="mtvnCWSZChgPwdResData" xmlns="mtvnCWSZChgPwdResData" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="SZChgPwdResData">

<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="mtvnCWCBPrpdAdjResData" xmlns="mtvnCWCBPrpdAdjResData" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="CBPrpdAdjResData">
		<xs:annotation>
		<xs:documentation>Cardbase Prepaid Adjustments (CB550) - This function is used to perform a debit or credit adjustment on a prepaid debit card.  Only active cards can be adjusted.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="E130013" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Card Number</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E130643" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Card Proxy ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E130487" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Credit Amount</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E130488" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Debit Amount</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E130486" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Reason Code</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E130582" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Balance Withdrawal Indicator</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E131150" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Detailed Transaction Description ID</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E202270" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Terminal Number</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E202271" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Terminal Sequence Number</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E202272" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Merchant Name</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E202273" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Merchant City and State</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="E202274" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Merchant Country</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ErrCde" type="xs:string" minOccurs="0"/>
				<xs:element name="ErrResnCde" type="xs:string" minOccurs="0"/>
				<xs:element name="ApplMsgLst" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ApplMsg" maxOccurs="unbounded">
								<xs:complexType>
									<xs:all>
										<xs:element name="ApplMsgApplId" type="xs:string" minOccurs="0"/>
										<xs:element name="ApplMsgNbr" type="xs:string" minOccurs="0"/>
										<xs:element name="ApplMsgTxt" type="xs:string" minOccurs="0"/>
										<xs:element name="ApplMsgErrInd" type="xs:string" minOccurs="0"/>
										<xs:element name="ApplMsgFld" type="xs:string" minOccurs="0"/>
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>

